{
  "name": "Claro_Prueba",
  "version": "1.0.0",
  "description": "Claro_Prueba",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf build",
    "lint": "eslint src || true",
    "test": "jest  --setupTestFrameworkScriptFile=raf/polyfill",
    "dev": "webpack-dashboard -t 'Webpack Dashboard' -- webpack-dev-server",
    "build": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "deploy": "npm run build && cp now.json ./build && now ./build --public",
    "alias": "cp now.json ./build && cd ./build && now alias",
    "deploy-production": "npm run build && cp now.production.json ./build/now.json && now ./build",
    "alias-production": "cp now.production.json ./build/now.json && cd ./build && now alias"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjonyivan/claro-prueba.git"
  },
  "keywords": [
    "react",
    "webpack3",
    "boilerplate",
    "redux",
    "sass"
  ],
  "author": "Jony Ivan C",
  "bugs": {
    "url": "https://github.com/cjonyivan/claro-prueba.git"
  },
  "homepage": "https://github.com/cjonyivan/claro-prueba",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.10",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-hot-loader": "^3.1.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "superagent": "^3.8.3"
  },
  "jest": { "moduleNameMapper": { "\\.(css|less)$": "<rootDir>/src/scss/styleMock.js" } }
}
